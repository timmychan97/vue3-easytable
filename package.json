{
  "name": "easytable",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@10.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/kohaiy/easytable#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohaiy/easytable.git"
  },
  "bugs": {
    "url": "https://github.com/kohaiy/easytable/issues"
  },
  "keywords": [],
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "docs": "pnpm --filter=docs dev",
    "docs:build": "pnpm --filter=docs build",
    "lint": "eslint .",
    "lint:err": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ci": "vitest run",
    "build:comp": "vite build",
    "clean": "rimraf dist packages/vue/libs docs/dist _build_gh_pages node_modules/.vite",
    "clean:deps": "rimraf node_modules packages/*/node_modules docs/node_modules pnpm-lock.yaml",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-angular": "^16.3.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "^2.0.0",
    "eslint": "^9.19.0",
    "husky": "^7.0.4",
    "jest-serializer-vue": "^3.1.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "lodash": "^4.17.21",
    "npm-run-all2": "^7.0.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^0.17.3",
    "vite": "^5.0.10",
    "vitest": "^1.2.0",
    "vue-tsc": "^2.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "resolutions": {
    "@babel/traverse": "7.25.0"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "eslint . --fix"
  }
}
