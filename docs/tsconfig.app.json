{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "rootDir": "../",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"],
    "noEmit": true
  },
  "include": ["env.d.ts", "vue-properties.d.ts", "src/**/*", "src/**/*.vue", "./auto-imports.d.ts", "../packages/**/*"],
  "exclude": ["src/**/__tests__/*"]
}
